      <script type="text/javascript">
      function calculate(x) 
      {
          var my1 = x;
          var my2 = document.getElementById('r2').value;
          document.getElementById('MED-bup-tab').innerTEXT = (my1) * parseInt(my2); 
          return document.getElementById('MED-bup-tab').innerTEXT
          
